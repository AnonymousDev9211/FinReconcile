@model FinReconcile.Models.CompareResult


        <div class="row">           
            <div class="col-md-12">
                <table class="table table-striped table-hover table-responsive table-condensed">
                    <thead>
                        <tr>
                            <th>@Model.ClientMarkOffFile</th>                            
                            <th></th>
                            <th></th>
                            <th>@Model.BankMarkOffFile</th>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <thead>
                        <tr>                            
                            <th>Date</th>
                            <th>Amount</th>
                            <th>Reference</th>                                               
                            <th>Date</th>
                            <th>Amount</th>
                            <th>Reference</th>
                            
                        </tr>
                    </thead>
                    <tbody>
 
                            @for (int i = 0; i < Model.Result.NotMatchedItems.Count; i++)
                            {
                                var clientTrans = Model.Result.NotMatchedItems[i].ClientTransaction;
                                var bankTrans = Model.Result.NotMatchedItems[i].BankTransaction;
                                <tr>
                                   @if (clientTrans != null)
                                   {
                                        <td class="@(bankTrans!=null&&clientTrans.Date==bankTrans.Date?"":"warning")">@clientTrans.Date.ToString()</td>
                                        <td class="@(bankTrans!=null&&clientTrans.Amount==bankTrans.Amount?"":"warning")">@clientTrans.Amount.ToString()</td>
                                        <td class="@(bankTrans != null && clientTrans.WalletReference==bankTrans.WalletReference?"":"warning")">@clientTrans.WalletReference</td>                                       
                                   }
                                   else
                                   {
                                       <td></td>
                                    <td></td>
                                    <td></td>
                                    
                                   }
@if (bankTrans != null)
{
                                       <td>@bankTrans.Date.ToString()</td>
                                    <td>@bankTrans.Amount.ToString()</td>
                                    <td>@bankTrans.WalletReference</td>
                                    
}
else
{
    <td></td>
        <td></td>
                                    <td></td>
                                    
}
                                </tr>
                            }                                                                  
                    </tbody>
                </table> 
            </div>          
            
        </div>
        
