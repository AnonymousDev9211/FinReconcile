@model FinReconcile.Models.CompareResult


        <div class="row">           
            <div class="col-md-12">
                <table class="table table-striped table-hover table-responsive table-condensed">
                    <thead>
                        <tr>
                            <th>@Model.ClientMarkOffFile</th>                            
                            <th></th>
                            <th></th>
                            <th>@Model.TutukaMarkOffFile</th>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <thead>
                        <tr>                            
                            <th>Date</th>
                            <th>Amount</th>
                            <th>Reference</th>
                            <th>Id</th>
                            <th>Description</th>
                            <td>ProfileName</td>
                            <td>Narrative</td>
                            <td>Type</td>
                            <th>Date</th>
                            <th>Amount</th>
                            <th>Reference</th>
                            <th>Id</th>
                            <th>Description</th>
                            <td>ProfileName</td>
                            <td>Narrative</td>
                            <td>Type</td>
                        </tr>
                    </thead>
                    <tbody>
 
                            @for (int i = 0; i < Model.Result.NotMatchedItems.Count; i++)
                            {
                                var clientTrans = Model.Result.NotMatchedItems[i].ClientTransaction;
                                var tutukaTrans = Model.Result.NotMatchedItems[i].TutukaTransaction;
                                <tr>
                                   @if (clientTrans != null)
                                   {
                                        <td class="@(tutukaTrans!=null&&clientTrans.Date==tutukaTrans.Date?"":"warning")">@clientTrans.Date.ToString()</td>
                                        <td class="@(tutukaTrans!=null&&clientTrans.Amount==tutukaTrans.Amount?"":"warning")">@clientTrans.Amount.ToString()</td>
                                        <td class="@(tutukaTrans != null && clientTrans.WalletReference==tutukaTrans.WalletReference?"":"warning")">@clientTrans.WalletReference</td>
                                        <td class="@(tutukaTrans!=null&&clientTrans.Id==tutukaTrans.Id?"":"warning")">@clientTrans.Id</td>
                                        <td class="@(tutukaTrans!=null&&clientTrans.Description==tutukaTrans.Description?"":"warning")">@clientTrans.Description</td>
                                        <td class="@(tutukaTrans!=null&&clientTrans.ProfileName==tutukaTrans.ProfileName?"":"warning")">@clientTrans.ProfileName</td>
                                        <td class="@(tutukaTrans!=null&&clientTrans.Narrative==tutukaTrans.Narrative?"":"warning")">@clientTrans.Narrative</td>                                        
                                        <td class="@(tutukaTrans!=null&&clientTrans.Type==tutukaTrans.Type?"":"warning")">@clientTrans.Type</td>
                                   }
                                   else
                                   {
                                       <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                        <td></td>
                                        <td></td>
                                   }
@if (tutukaTrans != null)
{
                                       <td>@tutukaTrans.Date.ToString()</td>
                                    <td>@tutukaTrans.Amount.ToString()</td>
                                    <td>@tutukaTrans.WalletReference</td>
                                    <td>@tutukaTrans.Id</td>
                                    <td>@tutukaTrans.Description</td>
                                        <td>@tutukaTrans.ProfileName</td>
                                        <td>@tutukaTrans.Narrative</td>
                                        <td>@tutukaTrans.Type</td>
}
else
{
    <td></td>
        <td></td>
                                    <td></td>
                                    <td></td>
                                        <td></td>
                                        <td></td>
}
                                </tr>
                            }                                                                  
                    </tbody>
                </table> 
            </div>          
            
        </div>
        
