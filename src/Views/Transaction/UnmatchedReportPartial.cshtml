@model FinReconcile.Models.CompareResult


        <div class="row">           
            <div class="col-md-12">
                <table class="table table-striped table-hover ">
                    <thead>
                        <tr>
                            <th>@Model.ClientMarkOffFile</th>                            
                            <th></th>
                            <th></th>
                            <th>@Model.TutukaMarkOffFile</th>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <thead>
                        <tr>                            
                            <th>Date</th>
                            <th>Amount</th>
                            <th>Reference</th>
                            <th>Date</th>
                            <th>Amount</th>
                            <th>Reference</th>
                        </tr>
                    </thead>
                    <tbody>
                            @for (int i = 0; i < Model.Result.NotMatchedItems.Count; i++)
                            {
                                var clientTrans = Model.Result.NotMatchedItems[i].ClientTransaction;
                                var tutukaTrans = Model.Result.NotMatchedItems[i].TutukaTransaction;
                                <tr>
                                   @if (clientTrans != null)
                                   {
                                        <td>@clientTrans.Date.ToString()</td>
                                        <td>@clientTrans.Amount.ToString()</td>
                                        <td>@clientTrans.WalletReference</td>
                                   }else
                                   {
                                       <td></td>
                                    <td></td>
                                    <td></td>
                                   }
@if (tutukaTrans != null)
{
                                       <td>@tutukaTrans.Date.ToString()</td>
                                    <td>@tutukaTrans.Amount.ToString()</td>
                                    <td>@tutukaTrans.WalletReference</td>
}
else
{
    <td></td>
        <td></td>
                                    <td></td>
}
                                </tr>
                            }                                                                  
                    </tbody>
                </table> 
            </div>          
            
        </div>
        
